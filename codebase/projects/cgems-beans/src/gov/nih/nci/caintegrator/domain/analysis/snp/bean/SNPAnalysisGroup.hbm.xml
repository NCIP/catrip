<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	

<hibernate-mapping package="gov.nih.nci.caintegrator.domain.analysis.snp.bean">
   <class name="SNPAnalysisGroup"
    	table="SNP_ANALYSIS_GROUP"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           type="java.lang.Long"
           column="ANA_GRP_ID">
           <generator class="assigned" />
       </id>

        <!-- memberCount-->
         <property name="memberCount"
                 type="java.lang.Integer"
                 column="MEMBER_COUNT"
                 insert="false"
                 update="false" />

        <!-- name -->
       <property name="name" 
                 type="java.lang.String" 
                 column="ANA_GRP_NAME" 
                 insert="false" 
                 update="false" />
                 
       <!-- description -->                 
       <property name="description" 
                 type="java.lang.String" 
                 column="ANA_GRP_DESCRIPTION" 
                 insert="false" 
                 update="false" />        
                 
     
	
     
         
      <!-- here is for m-m relationships -->  
      
       <set name="studyParticipantCollection" table="STDPT_ANALYSIS_GRP_AS" lazy="true">
         <cache usage="read-write" />
         <key column="ANA_GRP_ID" />
         <many-to-many class="gov.nih.nci.caintegrator.domain.study.bean.StudyParticipant"
                column="PARTICIPANT_DID" />
      </set>            
 
    
    <!--  m-1 relationship -->
    
   <many-to-one name="snpAssociationAnalysis"
                class="gov.nih.nci.caintegrator.domain.analysis.snp.bean.SNPAssociationAnalysis"
           column="ASSO_ANALYSIS_ID"  />
  
      
     	
      
      
      
	</class>
</hibernate-mapping>