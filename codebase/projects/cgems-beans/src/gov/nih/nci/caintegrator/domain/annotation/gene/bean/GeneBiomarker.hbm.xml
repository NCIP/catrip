<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	

<hibernate-mapping package="gov.nih.nci.caintegrator.domain.annotation.gene.bean">
   <class name="GeneBiomarker" 
    	table="GENE_DIM"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           type="java.lang.String" 
           column="GENE_ID">
           <generator class="assigned" />
       </id>
       
       
        <!-- chromosome -->        
       <property name="chromosome" 
                 type="java.lang.String" 
                 column="CHROMOSOME" 
                 insert="false" 
                 update="false" />
     
                 
        <!-- hugoGeneSymbol -->        
       <property name="hugoGeneSymbol" 
                 type="java.lang.String" 
                 column="FEATURE_NAME" 
                 insert="false" 
                 update="false" />
                 
       <!-- startPhyscialLocation -->                 
       <property name="startPhyscialLocation" 
                 type="java.lang.Long" 
                 column="CHROMOSOME_START" 
                 insert="false" 
                 update="false" />    
                 
         <!-- endPhysicalLocation -->                 
       <property name="endPhysicalLocation" 
                 type="java.lang.Long" 
                 column="CHROMOSOME_END" 
                 insert="false" 
                 update="false" />    
                 
        <!-- cytoband -->        
      <!--   <property name="cytoband" 
                 type="java.lang.String" 
                 column="CYTOBAND" 
                 insert="false" 
                 update="false" />
       -->                  
     
       <!-- entrezGeneID -->                 
        <!--  <property name="entrezGeneID" 
                 type="java.lang.String" 
                 column="ENTREZ_GENE_ID" 
                 insert="false" 
                 update="false" />     
         -->               
     
            
     
     
      <!-- here is for m-m relationships -->      
      
      <set name="snpAnnotationCollection" table="GENE_SNP_ASSO" >
         <cache usage="read-write" />
         <key column="GENE_ID" />
         <many-to-many class="gov.nih.nci.caintegrator.domain.annotation.snp.bean.SNPAnnotation" column="SNPANNO_ID" />
      </set>                         
	</class>
</hibernate-mapping>