<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	

<hibernate-mapping package="gov.nih.nci.caintegrator.domain.annotation.snp.bean">
   <class name="SNPPanel" 
    	table="SNP_PANEL"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           type="java.lang.Long" 
           column="SNP_PANEL_ID">
           <generator class="assigned" />
       </id>
       
      <property name="assayCount"
                type="java.lang.Integer"
                column="ASSAY_COUNT"
                insert="false"
                update="false" />

        <!-- description -->        
       <property name="description" 
                 type="java.lang.String" 
                 column="PANEL_DESCRIPTION" 
                 insert="false" 
                 update="false" />
                 
       <!-- name -->                 
       <property name="name" 
                 type="java.lang.String" 
                 column="SNP_PANEL_NAME" 
                 insert="false" 
                 update="false" />                 
     
                 
        <!-- technology -->                 
       <property name="technology" 
                 type="java.lang.String" 
                 column="TECHNOLOGY" 
                 insert="false" 
                 update="false" />
	    
	    <!-- vendor -->                 
       <property name="vendor" 
                 type="java.lang.String" 
                 column="VENDOR" 
                 insert="false" 
                 update="false" />
                 
        <!-- vendorPanelId-->                 
        <property name="vendorPanelId" 
                 type="java.lang.String" 
                 column="VENDOR_PANEL_ID" 
                 insert="false" 
                 update="false" />  
                 
        <!-- version-->                 
        <property name="version" 
                 type="java.lang.String" 
                 column="VERSION" 
                 insert="false" 
                 update="false" />     
                 
                 
<!--
      TODO: Fix this
      here is for m-m relationships

      <set name="studyCollection" table="STUDY_PANEL_ASSO" lazy="true"> 
        <cache usage="read-write" />
        <key column="SNP_PANEL_ID" />
         <many-to-many class="gov.nih.nci.caintegrator.domain.study.Study" column="STUDY_NAME" />
      </set>         
     
-->
     <set name="snpAssayCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="SNP_PANEL_ID" />
      <one-to-many class="gov.nih.nci.caintegrator.domain.annotation.snp.bean.SNPAssay" />
     </set>     
   
   
      
      
      
	</class>
</hibernate-mapping>