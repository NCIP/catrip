<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Object Mapping DTD Version 1.0//EN" "http://www.castor.org/mapping.dtd">

<mapping>
  <class name="gov.nih.nci.catrip.cagrid.catripquery.server.QueryDb">
    <map-to xml="QueryDb" ns-uri="gme://caCORE.cabig/3.0/gov.nih.nci.catrip.cagrid.catripquery.server" />
    <field name="id" type="int">
      <bind-xml name="id" node="attribute" />
    </field>
    <field name="firstName" type="string">
      <bind-xml name="firstName" node="attribute" />
    </field>
    <field name="lastName" type="string">
      <bind-xml name="lastName" node="attribute" />
    </field>
    <field name="userName" type="string">
      <bind-xml name="userName" node="attribute" />
    </field>
    <field name="description" type="string">
      <bind-xml name="description" node="attribute" />
    </field>
    <field name="name" type="string">
      <bind-xml name="name" node="attribute" />
    </field>
    <field name="source" type="string">
      <bind-xml name="source" node="attribute" />
    </field>
    <field name="version" type="string">
      <bind-xml name="version" node="attribute" />
    </field>
    <field name="instance" type="string">
      <bind-xml name="instance" node="attribute" />
    </field>
     <field name="dcql" type="string">
      <bind-xml name="dcql" node="attribute" />
    </field>
  <field name="creationDate" type="date">
      <bind-xml name="creationDate" node="attribute" />
    </field>
    <field name="dateUpdated" type="date">
      <bind-xml name="dateUpdated" node="attribute" />
    </field>
 	  <field name="dcqlCollection" type="gov.nih.nci.catrip.cagrid.catripquery.server.DcqlDb" collection="set">
         <bind-xml name="dcqlCollection"/>
    </field>    
  </class>
</mapping>

