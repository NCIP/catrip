<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="gov.nih.nci.ctom.domain.Neoplasm" table="neoplasm">
 		<id name="id" type="int" column="id">
        	<generator class="sequence">
                <param name="sequence">neoplasm_Seq</param>
        	</generator>
		</id>
        <property name="cellTypeCode" column="cell_Type_Code" type="string"/>

    </class>

</hibernate-mapping>
