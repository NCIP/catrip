<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="edu.pitt.cabig.cae.domain.general.AnnotationSet" table="annotation_set">
        <id
            name="id"
            column="ID"
            type="java.lang.Long"
        >
            <generator class="native">
             </generator>
        </id>

       	<joined-subclass name="edu.pitt.cabig.cae.domain.general.CancerTNMFinding" table="breast_Cancer_TNMF_Findings">
       		<key column="id"/>
      		<property name="category" column="category" type="string"/>
        	<property name="primaryTumorFinding" column="primary_Tumor_Finding" type="string"/>
        	<property name="regionalLymphNodesFinding" column="regional_Nodes_Finding" type="string"/>
        	<property name="distantMetastasisFinding" column="distant_Met_Finding" type="string"/>
      		<joined-subclass name="edu.pitt.cabig.cae.domain.breast.BreastCancerTNMFinding" table="breast_Cancer_TNMF_Findings">
       			<key column="id"/>
       			<property name="numberLymphNodesExamined" type="java.lang.Integer" column="NUMBER_NODES_EXAMINED"/>
        		<property name="numberLymphNodesInvolved" type="java.lang.Integer" column="NUMBER_NODES_INVOLVED"/>
            	<bag name="metastasisAnatomicSite" table="BREAST_MET_ANATOMIC_SITES" lazy="false" cascade="none">
            		<key column="ID"></key>
		            <element column="METASTASIS_ANATOMIC_SITE" type="java.lang.String" not-null="false" unique="false"/>
				</bag>
         		
        		<property name="otherMetastaticAnatomicSite" type="java.lang.String" column="OTHER_MET_ANATOMIC_SITE"/>
        		<property name="category" type="java.lang.String" column="CATEGORY"/>
        		<property name="primaryTumorFinding" type="java.lang.String" column="PRIMARY_TUMOR_FINDING" />
        		<property name="regionalLymphNodesFinding" type="java.lang.String" column="REGIONAL_NODES_FINDING"/>
        		<property name="distantMetastasisFinding" type="java.lang.String" column="DISTANT_MET_FINDING"/>
       	 	</joined-subclass>
       	 	
		</joined-subclass>
   		<!-- <joined-subclass name="edu.pitt.cabig.cae.domain.general.ThreeDimensionalSize" table="Three_Dimensional_Sizes">
       		<key column="id"/>
        	<property name="greatestDimension" type="java.lang.Integer" update="true" insert="true"
            column="GREATEST_DIMENSION" />

        	<property name="additionalDimensionY" type="java.lang.Integer" update="true" insert="true"
            column="ADDITIONAL_DIMENSION_Y" />

        	<property name="additionalDimensionZ" type="java.lang.Integer" update="true" insert="true"
            column="ADDITIONAL_DIMENSION_Z" />

        	<property name="MVR" type="java.lang.String" update="true" insert="true"
            column="MVR" />
        </joined-subclass>
-->
       	<joined-subclass name="edu.pitt.cabig.cae.domain.general.AdditionalFindings" table="Additional_Findings">
       		<key column="id"/>
      		<property name="otherFindings" column="other_Findings" type="string"/>
		</joined-subclass>
       	
      	<joined-subclass name="edu.pitt.cabig.cae.domain.general.NeoplasmHistopathologicType" table="breast_neo_hstopthlgc_types">
       		<key column="id"/>
         	<property name="name" type="java.lang.String" update="true"  insert="true"
            	column="name" />
        	<property name="nameMVR" type="java.lang.String" update="true" insert="true"
            	column="name_MVR" />
        	<property name="otherName" type="java.lang.String" update="true"
            	insert="true" column="other_name" />

		</joined-subclass> 
       	<joined-subclass name="edu.pitt.cabig.cae.domain.general.Neoplasm" table="INVASIVE_BREAST_CARCINOMA">
       		<key column="id"/>
         <!--	<property name="name" type="java.lang.String" update="true"  insert="true"
            	column="name" />
        	<property name="nameMVR" type="java.lang.String" update="true" insert="true"
            	column="name_MVR" />
        	<property name="otherName" type="java.lang.String" update="true"
            	insert="true" column="other_name" />
-->

		</joined-subclass> 
		   <joined-subclass name="edu.pitt.cabig.cae.domain.breast.InvasiveBreastCarcinoma" table="INVASIVE_BREAST_CARCINOMA">
				<key column="id"/>
       

        <bag
            name="location"
            table="BREAST_LOCATIONS"
            lazy="false"
            cascade="none"
        >

            <key
                column="ID"
            >
            </key>

            <element
                column="LOCATION"
                type="java.lang.String"
                not-null="false"
                unique="false"
            />

      </bag>

        <property
            name="locationMVR"
            type="java.lang.String"
            update="true"
            insert="true"
            column="LOCATION_MVR"
        />

        <property
            name="venousLymphaticInvasion"
            type="java.lang.String"
            update="true"
            insert="true"
            column="VENOUS_LYMPHATIC_INVASION"
        />

        <bag
            name="microcalcificationLocation"
            table="BREAST_MC_LOCATIONS"
            lazy="false"
            cascade="none"
        >

            <key
                column="ID"
            >
            </key>

            <element
                column="MICROCALCIFICATION_LOCATION"
                type="java.lang.String"
                not-null="false"
                unique="false"
            />

      </bag>

 
    </joined-subclass>
		
     </class>

</hibernate-mapping>
