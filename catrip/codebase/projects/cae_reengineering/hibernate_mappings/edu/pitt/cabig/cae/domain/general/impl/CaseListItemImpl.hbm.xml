<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.pitt.cabig.cae.domain.general.impl">
  <class name="CaseListItemImpl" table="CASE_LIST_ITEM" mutable="true">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
      <generator class="native" />
    </id>
    <property name="participantId" type="java.lang.Long" column="PARTICIPANT_ID" />
    <property name="userId" type="java.lang.Long" column="USER_ID" />
    <property name="diseaseType" type="java.lang.String" column="DISEASE_TYPE" />
  </class>
</hibernate-mapping>

