<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.pitt.cabig.cae.domain.general.impl">
  <class name="CAEStagedXMLImpl" table="CAE_STAGED_XML" mutable="true">
    <id name="id" type="java.lang.Long" column="id">
      <generator class="native" />
    </id>
    <property name="importSessionId" type="java.lang.String" column="importSessionId" />
    <property name="sourceId" type="java.lang.Integer" column="sourceId" />
    <property name="firstName" type="java.lang.String" column="firstName" />
    <property name="lastName" type="java.lang.String" column="lastName" />
    <property name="accessionId" type="java.lang.String" column="accessionId" />
    <property name="organId" type="java.lang.Integer" column="organId" />
    <property name="xmlRecord" type="edu.pitt.cabig.cae.util.OracleClobType" column="xmlRecord" />
    <property name="importDate" type="java.util.Date" column="importDate" />
  </class>
</hibernate-mapping>

