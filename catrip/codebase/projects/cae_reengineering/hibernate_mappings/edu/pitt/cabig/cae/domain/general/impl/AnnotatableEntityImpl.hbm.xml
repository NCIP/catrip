<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.pitt.cabig.cae.domain.general.impl">
  <class name="AnnotatableEntityImpl" table="ANNOTATABLE_ENTITY" mutable="true">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
      <generator class="native" />
    </id>
    <set name="annotationEventParametersCollection" lazy="false" cascade="save-update" inverse="true">
      <cache usage="read-write" />
      <key column="ANNOTATABLE_ENTITY_ID" />
      <one-to-many class="edu.pitt.cabig.cae.domain.general.impl.AnnotationEventParametersImpl" />
    </set>
    <joined-subclass name="edu.pitt.cabig.cae.domain.general.impl.AccessionImpl" table="ACCESSION">
      <key column="ANNOTATABLE_ENTITY_ID" />
      <property name="diseaseType" type="java.lang.String" column="DISEASE_TYPE" />
      <set name="specimenCollection" lazy="false" cascade="save-update" inverse="true">
        <cache usage="read-write" />
        <key column="ACCESSION_ID" />
        <one-to-many class="edu.pitt.cabig.cae.domain.general.impl.SpecimenImpl" />
      </set>
		<many-to-one name="participant" class="edu.pitt.cabig.cae.domain.general.impl.ParticipantImpl" column="PARTICIPANT_ID" lazy="false" />
    </joined-subclass>
    <joined-subclass name="edu.pitt.cabig.cae.domain.general.impl.ParticipantImpl" table="PARTICIPANT">
      <key column="ANNOTATABLE_ENTITY_ID" />
      <set name="accessionCollection" lazy="false" cascade="save-update" inverse="true">
        <cache usage="read-write" />
        <key column="PARTICIPANT_ID" />
        <one-to-many class="edu.pitt.cabig.cae.domain.general.impl.AccessionImpl" />
      </set>
    </joined-subclass>
    <joined-subclass name="edu.pitt.cabig.cae.domain.general.impl.SpecimenImpl" table="SPECIMEN">
      <key column="ANNOTATABLE_ENTITY_ID" />
      <set name="specimenCollection" lazy="false" cascade="save-update" inverse="true">
        <cache usage="read-write" />
        <key column="PARENT_SPECIMEN_ID" />
        <one-to-many class="edu.pitt.cabig.cae.domain.general.impl.SpecimenImpl" />
      </set>
      <many-to-one name="accession" class="edu.pitt.cabig.cae.domain.general.impl.AccessionImpl" column="ACCESSION_ID" lazy="false" />
      <many-to-one name="specimen" class="edu.pitt.cabig.cae.domain.general.impl.SpecimenImpl" column="PARENT_SPECIMEN_ID" lazy="false" />
    </joined-subclass>
  </class>
</hibernate-mapping>

