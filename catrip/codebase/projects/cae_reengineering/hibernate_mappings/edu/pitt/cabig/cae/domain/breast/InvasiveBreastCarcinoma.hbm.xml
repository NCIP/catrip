<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="edu.pitt.cabig.cae.domain.breast.InvasiveBreastCarcinoma"
        table="INVASIVE_BREAST_CARCINOMA"
    >

        <id
            name="id"
            column="ID"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-InvasiveBreastCarcinoma.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <bag
            name="location"
            table="BREAST_LOCATIONS"
            lazy="false"
            cascade="none"
        >

            <key
                column="ID"
            >
            </key>

            <element
                column="LOCATION"
                type="java.lang.String"
                not-null="false"
                unique="false"
            />

      </bag>

        <property
            name="locationMVR"
            type="java.lang.String"
            update="true"
            insert="true"
            column="LOCATION_MVR"
        />

        <property
            name="venousLymphaticInvasion"
            type="java.lang.String"
            update="true"
            insert="true"
            column="VENOUS_LYMPHATIC_INVASION"
        />

        <bag
            name="microcalcificationLocation"
            table="BREAST_MICROCALCIFICATION_LOCATIONS"
            lazy="false"
            cascade="none"
        >

            <key
                column="ID"
            >
            </key>

            <element
                column="MICROCALCIFICATION_LOCATION"
                type="java.lang.String"
                not-null="false"
                unique="false"
            />

      </bag>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-InvasiveBreastCarcinoma.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
