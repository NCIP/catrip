<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.pitt.cabig.cae.domain.general.impl">
  <class name="AnnotationSetImpl" table="ANNOTATION_SET" mutable="true" lazy="false">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
      <generator class="native" />
    </id>
    <many-to-one name="annotationEvent" class="edu.pitt.cabig.cae.domain.general.impl.AnnotationEventParametersImpl" column="ANNOTATION_EVENT_PARM_ID" lazy="false" />
	 <property name="annotationSetTypeClassName" column="type"/>
	 <set name="annotations" lazy ="false" inverse="true" cascade="save-update">
		 <key column="ANNOTATION_SET_ID"/>
		 <one-to-many class="edu.pitt.cabig.cae.domain.general.impl.AnnotationImpl"/>
	 </set>
  </class>
</hibernate-mapping>
