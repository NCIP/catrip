        table="BREAST_NEOPLASM_HISTOPATHALOGIC_TYPES"
    >

        <id
            name="id"
            column="ID"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-InvasiveBreastCarcinomaNeoplasmHistologicType.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="NAME"
        />

        <property
            name="nameMVR"
            type="java.lang.String"
            update="true"
            insert="true"
            column="NAME_MVR"
        />

        <property
            name="otherName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="OTHER_NAME"
