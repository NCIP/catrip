<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.pitt.cabig.cae.domain.general.impl">
  <!-- copied from cae -->
  <class name="EventParametersImpl" table="EVENT_PARAMETERS" mutable="true">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="ID">
      <generator class="native" />
    </id>
    <property name="timeStamp" type="java.util.Date" column="TIME_STAMP" />
    <joined-subclass name="edu.pitt.cabig.cae.domain.general.impl.AnnotationEventParametersImpl" table="ANNOTATION_EVENT_PARAMETER">
      <key column="EVENT_PARAMETERS_ID" />
      <property name="source" type="java.lang.String" column="SOURCE" />
      <property name="sourceDate" type="java.util.Date" column="SOURCE_DATE" />
      <many-to-one name="annotatableEntity" class="edu.pitt.cabig.cae.domain.general.impl.AnnotatableEntityImpl" column="ANNOTATABLE_ENTITY_ID" lazy="false" />
      <set name="annotationSetCollection" lazy="false" cascade="save-update" inverse="true">
        <cache usage="read-write" />
        <key column="ANNOTATION_EVENT_PARM_ID" />
        <one-to-many class="edu.pitt.cabig.cae.domain.general.impl.AnnotationSetImpl" />
      </set>
    </joined-subclass>
  </class>
</hibernate-mapping>

