<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="gov.nih.nci.ctom.domain.ActivityRelationship" table="activity_relationship">
  		<id name="id" type="int" column="id">
        	<generator class="sequence">
                <param name="sequence">activity_relationship_seq</param>
        	</generator>
		</id>
        <property name="commentText" column="comment_Text" type="string"/>
        <property name="sequenceNumber" column="sequence_Number" type="int"/>
        <property name="typeCode" column="type_code" type="string"/>
     	<many-to-one name="activity_1" class="gov.nih.nci.ctom.domain.Activity" column="activity_id_1" cascade="save-update,persist"/>
 		<many-to-one name="activity_2" class="gov.nih.nci.ctom.domain.Activity" column="activity_id_2" cascade="save-update,persist"/>
     
    </class>

</hibernate-mapping>
