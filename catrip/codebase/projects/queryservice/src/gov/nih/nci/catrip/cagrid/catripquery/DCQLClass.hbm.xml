<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping auto-import="false">
    <class  name="gov.nih.nci.catrip.cagrid.catripquery.DCQLClass" lazy="false" table="DCQL_CLASS">
        <id name="id" column="ID" type="int">
			<generator class="sequence">
			    <param name="sequence">hibernate</param>
			</generator>
        </id>
        <property name="name" column="NAME" type="string"/>
		
		<set name="attributeCollection" inverse="false" cascade="all" >
			<key column="CLASS_ID"/>
			<one-to-many class="gov.nih.nci.catrip.cagrid.catripquery.DCQLAttribute"/>
		</set>
	
   </class>
</hibernate-mapping>