<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : SNPAssay.hbm.xml
    Created on : June 26, 2006
    Author     : Dana Zhang
    Description: Hibernate mapping file for SNPAssay
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	

<hibernate-mapping package="gov.nih.nci.caintegrator.domain.annotation.snp.bean">
   <class name="SNPAssay" 
    	table="SNP_ASSAY"
        lazy="true">
        
       <!-- id -->
       <id name="id" 
           type="java.lang.Long" 
           column="ASSAY_ID">
           <generator class="assigned" />
       </id>
       
       
        <!-- designAlleles -->        
       <property name="designAlleles" 
                 type="java.lang.String" 
                 column="DESIGN_ALLELES" 
                 insert="false" 
                 update="false" />
                 
       <!-- designScore -->                 
       <property name="designScore" 
                 type="java.lang.Float" 
                 column="DESIGN_SCORE" 
                 insert="false" 
                 update="false" />                 
     
                 
        <!-- designSequence -->                 
       <property name="designSequence" 
                 type="java.lang.String" 
                 column="DESIGN_SEQUENCE" 
                 insert="false" 
                 update="false" />
	    
	    <!-- designStrand -->                 
       <property name="designStrand" 
                 type="java.lang.String" 
                 column="DESIGN_STRAND" 
                 insert="false" 
                 update="false" />
                 
        <!-- status-->                 
        <property name="status" 
                 type="java.lang.String" 
                 column="STATUS" 
                 insert="false" 
                 update="false" />  
                 
        <!-- vendorAssayId-->                 
        <property name="vendorAssayId" 
                 type="java.lang.String" 
                 column="VENDOR_ASSAY_ID" 
                 insert="false" 
                 update="false" />    
                 
        <!-- version-->                 
        <property name="version" 
                 type="java.lang.String" 
                 column="VERSION" 
                 insert="false" 
                 update="false" />     
                 
                 
                  
         
     
    <!-- here is for 1-m relationships -->  
    
     <set name="genotypeFindingCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="ASSAY_ID" />
      <one-to-many class="gov.nih.nci.caintegrator.domain.finding.variation.germline.bean.GenotypeFinding" />
     </set>     

   <many-to-one name="snpPanel"
             class="gov.nih.nci.caintegrator.domain.annotation.snp.bean.SNPPanel"
             column="SNP_PANEL_ID"
             not-null="true" />

   <many-to-one name="snpAnnotation"
             class="gov.nih.nci.caintegrator.domain.annotation.snp.bean.SNPAnnotation"
             column="SNPANNO_ID"
             not-null="true" />

	</class>
</hibernate-mapping>