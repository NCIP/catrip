 <ns3:Services xsi:type="ns3:ServicesType" xmlns:ns3="gme://gov.nih.nci.cagrid.introduce/1/Services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns3:Service name="AuthenticationService" namespace="http://cagrid.nci.nih.gov/AuthenticationService" packageName="gov.nih.nci.cagrid.authentication" resourceFrameworkType="main" xsi:type="ns3:ServiceType">
   <ns4:Methods xsi:type="ns4:MethodsType" xmlns:ns4="gme://gov.nih.nci.cagrid.introduce/1/Methods">
    <ns4:Method isImported="true" isProvided="true" name="getServiceSecurityMetadata" xsi:type="ns4:MethodType">
     <ns4:Output qName="ns5:ServiceSecurityMetadata" isArray="false" isClientHandle="false" xmlns:ns5="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata.security"/>
     <ns4:ImportInformation inputMessage="ns6:GetServiceSecurityMetadataRequest" namespace="http://security.introduce.cagrid.nci.nih.gov/ServiceSecurity" outputMessage="ns6:GetServiceSecurityMetadataResponse" packageName="gov.nih.nci.cagrid.introduce.security.stubs" portTypeName="ServiceSecurityPortType" wsdlFile="ServiceSecurity.wsdl" xmlns:ns6="http://security.introduce.cagrid.nci.nih.gov/ServiceSecurity"/>
     <ns4:ProviderInformation providerClass="gov.nih.nci.cagrid.introduce.security.service.globus.ServiceSecurityProviderImpl"/>
    </ns4:Method>
    <ns4:Method isImported="false" isProvided="false" name="authenticate" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns7:Credential" isArray="false" name="credential" xmlns:ns7="http://cagrid.nci.nih.gov/1/authentication-service"/>
     </ns4:Inputs>
     <ns4:Output qName="ns8:SAMLAssertion" isArray="false" xmlns:ns8="http://cagrid.nci.nih.gov/1/authentication-service"/>
     <ns4:Exceptions>
      <ns4:Exception name="InvalidCredentialFault" qname="ns9:InvalidCredentialFault" xmlns:ns9="http://cagrid.nci.nih.gov/AuthenticationService/types"/>
      <ns4:Exception name="InsufficientAttributeFault" qname="ns10:InsufficientAttributeFault" xmlns:ns10="http://cagrid.nci.nih.gov/AuthenticationService/types"/>
      <ns4:Exception name="AuthenticationProviderFault" qname="ns11:AuthenticationProviderFault" xmlns:ns11="http://cagrid.nci.nih.gov/AuthenticationService/types"/>
     </ns4:Exceptions>
     <ns12:MethodSecurity xsi:type="ns12:MethodSecurity" xmlns:ns12="gme://gov.nih.nci.cagrid.introduce/1/Security">
      <ns12:SecuritySetting xsi:type="ns12:SecuritySetting">Custom</ns12:SecuritySetting>
      <ns12:TransportLevelSecurity xsi:type="ns12:TransportLevelSecurity">
       <ns12:CommunicationMethod xsi:type="ns12:CommunicationMethod">Privacy</ns12:CommunicationMethod>
      </ns12:TransportLevelSecurity>
      <ns12:RunAsMode xsi:type="ns12:RunAsMode">System</ns12:RunAsMode>
      <ns12:AnonymousClients xsi:type="ns12:AnonymousCommunication">Yes</ns12:AnonymousClients>
      <ns12:MethodAuthorization xsi:type="ns12:MethodAuthorization">
       <ns12:NoAuthorization xsi:type="ns12:NoAuthorization"/>
      </ns12:MethodAuthorization>
     </ns12:MethodSecurity>
    </ns4:Method>
   </ns4:Methods>
   <ns13:ResourcePropertiesList xsi:type="ns13:ResourcePropertiesListType" xmlns:ns13="gme://gov.nih.nci.cagrid.introduce/1/Resources">
    <ns13:ResourceProperty qName="ns14:ServiceMetadata" fileLocation="serviceMetadata.xml" populateFromFile="true" register="true" xsi:type="ns13:ResourcePropertyType" xmlns:ns14="gme://caGrid.caBIG/1.0/gov.nih.nci.cagrid.metadata"/>
   </ns13:ResourcePropertiesList>
   <ns15:ServiceSecurity xsi:type="ns15:ServiceSecurity" xmlns:ns15="gme://gov.nih.nci.cagrid.introduce/1/Security">
    <ns15:SecuritySetting xsi:type="ns15:SecuritySetting">Custom</ns15:SecuritySetting>
    <ns15:TransportLevelSecurity xsi:type="ns15:TransportLevelSecurity">
     <ns15:CommunicationMethod xsi:type="ns15:CommunicationMethod">Privacy</ns15:CommunicationMethod>
    </ns15:TransportLevelSecurity>
    <ns15:RunAsMode xsi:type="ns15:RunAsMode">System</ns15:RunAsMode>
    <ns15:AnonymousClients xsi:type="ns15:AnonymousCommunication">Yes</ns15:AnonymousClients>
    <ns15:ServiceAuthorization xsi:type="ns15:ServiceAuthorization">
     <ns15:NoAuthorization xsi:type="ns15:NoAuthorization"/>
    </ns15:ServiceAuthorization>
   </ns15:ServiceSecurity>
