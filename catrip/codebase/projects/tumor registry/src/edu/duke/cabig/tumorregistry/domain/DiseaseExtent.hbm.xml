<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="edu.duke.cabig.tumorregistry.domain">
	<class
		name="DiseaseExtent"
		table="DISEASE_EXTENT"
	>

		<id
			name="Id"
			type="long"
			column="ID"
		>
		</id>

		<property
			name="clinicalMetStage"
			column="CLINICAL_MET_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="clinicalNodeStage"
			column="CLINICAL_NODE_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="clinicalTumorStage"
			column="CLINICAL_TUMOR_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="pathologicMetStage"
			column="PATHOLOGIC_MET_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="pathologicNodeStage"
			column="PATHOLOGIC_NODE_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="pathologicTumorStage"
			column="PATHOLOGIC_TUMOR_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="tnmEdition"
			column="TNM_EDITION"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="tumorSize"
			column="TUMOR_SIZE"
			type="java.lang.Float"
			not-null="false"
			length="126"
		/>
		<property
			name="pathologicAJCCStage"
			column="PATHOLOGIC_AJCC_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="pathologicAJCCStageDescriptor"
			column="PATHOLOGIC_AJCC_STAGE_DESC"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="clinicalAJCCStage"
			column="CLINICAL_AJCC_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="clinicalAJCCStageDescriptor"
			column="CLINICAL_AJCC_STAGE_DESCRIPTOR"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="regionalNodesPositive"
			column="REGIONAL_NODES_POSITIVE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="regionalNodesExamined"
			column="REGIONAL_NODES_EXAMINED"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="bestAJCCStage"
			column="BEST_AJCC_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="bestSEERSummaryStage"
			column="BEST_SEER_SUMMARY_STAGE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="tumorMarker1"
			column="TUMOR_MARKER_1"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="tumorMarker2"
			column="TUMOR_MARKER_2"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="tumorMarker3"
			column="TUMOR_MARKER_3"
			type="string"
			not-null="false"
			length="50"
		/>
<!--
		<many-to-one
			name="Diagnosis"
			column="DIAGNOSIS_ID"
			class="Diagnosis"
			not-null="false"
		>
		</many-to-one>
	
-->
		<set name="distantSiteCollection" lazy="false"  inverse="false" cascade="all">
			<key column="disease_extent_id" not-null="false"/>
			<one-to-many class="DistantSite"/>
		</set>


	</class>	
</hibernate-mapping>